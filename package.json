  "devDependencies": {
    "@richrdkng/dmd-plugin-simple-nodejs-project": "^1.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@types/jest": "^26.0.23",
    "codecov": "^3.6.5",
    "gulp": "^4.0.2",
    "gulp-debug": "^4.0.0",
    "gulp-tap": "^2.0.0",
    "jest": "^26.0.1",
    "jsdoc-to-markdown": "^6.0.1",
    "semantic-release": "^17.0.6",
    "snazzy": "^8.0.0",
    "standard": "^14.3.3"
  },
  "standard": {
    "envs": [
      "jest"
    ]
  },
